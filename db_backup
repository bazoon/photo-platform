--
-- TOC entry 238 (class 1259 OID 18063)
-- Name: rates_jury; Type: VIEW; Schema: public; Owner: vn
--

CREATE VIEW public.rates_jury AS
 SELECT x_r.id,
    x_r.photowork_id,
    x_r.jury_id,
    x_r.rate_value,
    x_r.date_rate,
    x_j.user_id
   FROM public.rates x_r,
    public.juries x_j
  WHERE (x_r.jury_id = x_j.id);


ALTER TABLE public.rates_jury OWNER TO vn;

