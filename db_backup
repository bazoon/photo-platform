--
-- TOC entry 250 (class 1255 OID 26720)
-- Name: contests_switch_on(); Type: FUNCTION; Schema: public; Owner: vn
--

CREATE FUNCTION public.contests_switch_on() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF 	NEW.inworknow = TRUE THEN
		IF TG_OP = 'INSERT' THEN
			UPDATE contests set inworknow = FALSE
			WHERE salone_id = NEW.salone_id;
		END IF;
		IF TG_OP = 'UPDATE' AND OLD.inworknow != NEW.inworknow THEN			
			UPDATE contests set inworknow = FALSE
			WHERE salone_id = NEW.salone_id AND id != NEW.id;
		END IF;
	END IF;
	RETURN NEW;
END;
$$;


ALTER FUNCTION public.contests_switch_on() OWNER TO vn;

SET default_tablespace = '';

